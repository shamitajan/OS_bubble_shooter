#include<stdio.h>
#include<sys/types.h>
#include<unistd.h>
#include<stdlib.h>
#include<sys/wait.h>

void main()
{
  pid_t pid;
  if((pid=fork())<0)
      printf("Error in fork execution");
  else
  if(pid==0)
     printf("This is the child process id %d\n",getpid());
  else
     {
     sleep(10);
     printf("This is the parent id %d\n", getpid());
     printf("*****parent\n");
     system("ps -axj |tail");
     }
  exit(0);
}
